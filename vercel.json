{
    "rewrites": [
        {
            "source": "/swagger.html",
            "destination": "/swagger.html"
        },
        {
            "source": "/swagger.json",
            "destination": "/swagger.json"
        },
        {
            "source": "/swagger",
            "destination": "/swagger.html"
        },
        {
            "source": "/swager",
            "destination": "/swagger.html"
        },
        {
            "source": "/((?!swagger.html|swagger|swager|swagger.json).*)",
            "destination": "/index.html"
        }
    ]
}